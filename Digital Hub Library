FES:Textbox{
	Name = "TP To player Smoothly",
	Callback = function(text)
        local Person = game.Players:FindFirstChild(text)
        print(Person.UserId)
        local tweenService = game:GetService("TweenService")

        local changes = {
	        CFrame = Person.Character.HumanoidRootPart.CFrame
        }

        local tweenInfo = TweenInfo.new(
	        5,									-- Time taken for a full animation
	        Enum.EasingStyle.Cubic,			-- Animation Style
	        Enum.EasingDirection.Out,			-- Animation Type
	        0,									-- Number of repeats (-1 is infinite)
	        false,								-- Reverse? 
    	    0									-- Delay between animations
        )

        local tween = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, tweenInfo, changes)
        tween:Play()

    end
}

